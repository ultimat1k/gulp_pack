extends ../layouts/template
block description
	meta(name='description', content='description')
	meta(name='Keywords', content='keywords')
block favicon
	link(rel='icon', href='/favicon.png', type='image/x-icon')
	link(rel='shortcut icon', href='/favicon.ico', type='image/x-icon')

block title
	| Тестовая страничка
block body
	header
		// Navbar
		nav.navbar.navbar-expand-lg.navbar-dark.primary-color
			// Navbar brand
			a.navbar-brand(href='#') Navbar
			// Collapse button
			button.navbar-toggler(type='button', data-toggle='collapse', data-target='#basicExampleNav', aria-controls='basicExampleNav', aria-expanded='false', aria-label='Toggle navigation')
				span.navbar-toggler-icon
			// Collapsible content
			#basicExampleNav.collapse.navbar-collapse
				// Links
				ul.navbar-nav.mr-auto
					li.nav-item.active
						a.nav-link(href='#')
							| Home
							span.sr-only (current)
					li.nav-item
						a.nav-link(href='#') Features
					li.nav-item
						a.nav-link(href='#') Pricing
					// Dropdown
					li.nav-item.dropdown
						a#navbarDropdownMenuLink.nav-link.dropdown-toggle(data-toggle='dropdown', aria-haspopup='true', aria-expanded='false') Dropdown
						.dropdown-menu.dropdown-primary(aria-labelledby='navbarDropdownMenuLink')
							a.dropdown-item(href='#') Action
							a.dropdown-item(href='#') Another action
							a.dropdown-item(href='#') Something else here
				// Links
				form.form-inline
					.md-form.my-0
						input.form-control.mr-sm-2(type='text', placeholder='Search', aria-label='Search')
			// Collapsible content
		// /.Navbar
			