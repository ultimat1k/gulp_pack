extends ../layouts/template
block description
	meta(name='description', content='description')
	meta(name='Keywords', content='keywords')
block favicon
	link(rel='icon', href='/favicon.png', type='image/x-icon')
	link(rel='shortcut icon', href='/favicon.ico', type='image/x-icon')

block title
	| Тестовая страничка
block body
	header.st-navbar
		nav.navbar.navbar-default.navbar-fixed-top.clearfix(role='navigation')
			.container
				.navbar-header
					button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#sept-main-nav')
						span.sr-only Toggle navigation
						span.icon-bar
						span.icon-bar
						span.icon-bar
					a.navbar-brand(href='#')
						img.img-responsive(src='/photos/logo3.png', alt='')
				#sept-main-nav.collapse.navbar-collapse.main-nav
					ul.nav.navbar-nav.navbar-right
						li.active
							a(href='#home') Начало
						li
							a(href='#about') О нас
						li
							a(href='#service') Сервис
						li
							a(href='#portfolio') Портфолио
						li
							a(href='#pricing') Услуги
						li
							a(href='#blog') Блог
						li
							a(href='#contact') Контакты